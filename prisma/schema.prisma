datasource db {
  provider = "sqlite"
  url      = "file:../.wrangler/state/d1/DB.sqlite3"
}

model Visit {
  visitToken      String   @unique
  visitorToken    String
  longitude       Float?
  latitude        Float?
  continent       String?
  country         String?
  city            String?
  region          String?
  regionCode      String?
  postalCode      String?
  timezone        String?
  ip              String?
  referrer        String?
  referringDomain String?
  userAgent       String?
  landingPage     String?
  browser         String?
  browserVersion  String?
  os              String?
  osVersion       String?
  deviceType      String?
  deviceModel     String?
  deviceVendor    String?
  utmSource       String?
  utmMedium       String?
  utmTerm         String?
  utmContent      String?
  utmCampaign     String?
  startedAt       DateTime
  events          Event[]
}

model Event {
  id              Int      @id @default(autoincrement())
  visitToken      String
  visit           Visit    @relation(fields: [visitToken], references: [visitToken])
  key             String
  value           String?
  host            String?
  hostname        String?
  href            String?
  orign           String?
  pathname        String?
  port            String?
  protcol         String?
  search          String?
  longitude       Float?
  latitude        Float?
  continent       String?
  country         String?
  city            String?
  region          String?
  regionCode      String?
  postalCode      String?
  timezone        String?
  ip              String?
  referrer        String?
  referringDomain String?
  userAgent       String?
  landingPage     String?
  browser         String?
  browserVersion  String?
  os              String?
  osVersion       String?
  deviceType      String?
  deviceModel     String?
  deviceVendor    String?
  time            DateTime
}
